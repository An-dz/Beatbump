{
	"name": "beatbump-app",
	"version": "2.2.0",
	"scripts": {
		"dev": "npm run sync && vite dev --host",
		"bb:build": "node ./scripts/build.cjs build",
		"cfw:deploy": "wrangler publish",
		"vercel:deploy": "vercel deploy --prod",
		"netlify:deploy": "netlify deploy --prod",
		"preview": "vite preview --host",
		"build": "vite build",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"sync": "svelte-kit sync",
		"validate": "npm run format && npm run lint ",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint ."
	},
	"devDependencies": {
		"@babel/core": "^7.18.6",
		"@playwright/test": "^1.29.2",
		"@snuffydev/fast-cli": "^0.0.1",
		"@sveltejs/adapter-cloudflare": "^4.7.2",
		"@sveltejs/adapter-cloudflare-workers": "^2.0.0",
		"@sveltejs/adapter-netlify": "^4.3.3",
		"@sveltejs/adapter-node": "^5.2.2",
		"@sveltejs/adapter-vercel": "^5.4.3",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@swc/core": "^1.2.215",
		"@types/node": "^20.0.0",
		"@typescript-eslint/eslint-plugin": "^5.46.1",
		"@typescript-eslint/parser": "^5.46.1",
		"autoprefixer": "^10.4.7",
		"babel-loader": "^9.1.3",
		"cssnano": "^7.0.6",
		"cssnano-preset-default": "^7.0.6",
		"dashjs": "^4.4.1",
		"dotenv": "^16.0.1",
		"esbuild": "^0.16.8",
		"eslint": "^8.30.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-typescript": "^3.4.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"hls.js": "^1.1.5",
		"peerjs": "^1.4.6",
		"postcss": "^8.4.14",
		"postcss-load-config": "^6.0.1",
		"prettier": "^2.8.1",
		"prettier-plugin-svelte": "^2.10.1",
		"sass": "^1.53.0",
		"svelte": "^4.2.19",
		"svelte-check": "^4.0.1",
		"svelte-preprocess": "^6.0.2",
		"ts-node": "^10.9.1",
		"tslib": "^2.4.0",
		"typescript": "^5.5.4",
		"vite": "^5.4.3",
		"vitest": "^1.0.0"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/commissioner": "^4.5.7",
		"svelte-migrate": "^1.4.5"
	}
}
