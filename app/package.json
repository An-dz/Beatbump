{
	"name": "beatbump-app",
	"version": "2.2.0",
	"scripts": {
		"dev": "npm run sync && vite dev --host",
		"bb:build": "node ./scripts/build.cjs build",
		"cfw:deploy": "wrangler publish",
		"vercel:deploy": "vercel deploy --prod",
		"netlify:deploy": "netlify deploy --prod",
		"preview": "vite preview --host",
		"build": "vite build",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"sync": "svelte-kit sync",
		"validate": "npm run format && npm run lint ",
		"format": "prettier --plugin-search-dir . --write .",
		"lint": "prettier --plugin-search-dir . --check . && eslint --fix ."
	},
	"devDependencies": {
		"@babel/core": "^7.21.5",
		"@playwright/test": "^1.33.0",
		"@snuffydev/fast-cli": "^0.0.1",
		"@sveltejs/adapter-cloudflare": "^2.2.1",
		"@sveltejs/adapter-cloudflare-workers": "^1.1.1",
		"@sveltejs/adapter-netlify": "^2.0.7",
		"@sveltejs/adapter-node": "^1.2.4",
		"@sveltejs/adapter-vercel": "^2.4.3",
		"@sveltejs/kit": "^1.15.9",
		"@swc/core": "^1.3.56",
		"@types/node": "^18.16.3",
		"@typescript-eslint/eslint-plugin": "^5.59.1",
		"@typescript-eslint/parser": "^5.59.1",
		"autoprefixer": "^10.4.14",
		"babel-loader": "^9.1.2",
		"cssnano": "^6.0.1",
		"cssnano-preset-default": "^6.0.1",
		"dashjs": "^4.6.0",
		"dotenv": "^16.0.3",
		"esbuild": "^0.17.18",
		"eslint": "^8.39.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-import-resolver-typescript": "^3.5.5",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-svelte": "^2.27.2",
		"hls.js": "^1.4.0",
		"peerjs": "^1.4.7",
		"postcss": "^8.4.23",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.0",
		"sass": "^1.62.1",
		"svelte": "^3.58.0",
		"svelte-check": "^3.2.0",
		"svelte-preprocess": "^5.0.3",
		"ts-node": "^10.9.1",
		"tslib": "^2.5.0",
		"typescript": "^5.0.4",
		"vite": "^4.3.3",
		"vitest": "^0.30.1"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/commissioner": "^4.5.10"
	}
}
